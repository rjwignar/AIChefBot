# MONGODB ENV
DB_USER=YOUR_MONGODB_ATLAS_ADMIN_NAME
DB_PASS=YOUR_MONGODB_ATLAS_PASSWORD

# AWS Amazon User Pool ID
AWS_COGNITO_POOL_ID=YOUR_USER_POOL_ID
 
# AWS Amazon Cognito Host UI Domain
AWS_COGNITO_HOSTED_UI_DOMAIN=YOUR_COGNITO_UI_DOMAIN

# AWS Amazon Cognito Client App ID
AWS_COGNITO_CLIENT_ID=YOUR_COGNITO_CLIENT_ID
 
# OAuth Sign-In Redirect URL (DO NOT MODIFY)
OAUTH_SIGN_IN_REDIRECT_URL=http://localhost:3000/api/auth/callback/cognito
# OAuth Sign-Out Redirect URL (DO NOT MODIFY)
OAUTH_SIGN_OUT_REDIRECT_URL=http://localhost:3000
 
# AWS Amazon Cognito Logout Endpoint URL (DO NOT MODIFY)
OAUTH_SIGN_OUT_ENDPOINT_URL=${AWS_COGNITO_HOSTED_UI_DOMAIN}/logout?

# AWS Amazon Cognito Issuer (DO NOT MODIFY)
AWS_COGNITO_ISSUER=https://cognito-idp.us-east-1.amazonaws.com/${AWS_COGNITO_POOL_ID}

# NextAuthJS secret (generated using openssl rand)
NEXTAUTH_SECRET=YOUR_NEXTAUTH_SECRET

# OpenAI API Key
NEXT_PUBLIC_OPENAI_API_KEY=YOUR_OPENAI_API_KEY

# Cloudinary API Key
NEXT_PUBLIC_CLOUDINARY_API_KEY=YOUR_CLOUDINARY_API_KEY

# Cloudinary API Secret (not API Key)
CLOUDINARY_API_SECRET=YOUR_CLOUDINARY_API_SECRET

# Cloudinary Cloud Name
CLOUDINARY_CLOUD_NAME=YOUR_CLOUDINARY_CLOUD_NAME

# OPTIONAL
# KEEP THIS LINE TO DISABLE IMAGE GENERATION
GENERATE_IMAGES=OFF